                 ;=======================================;
                 
                 
                 
                 
                 
                 ;#######################################;
                 ;=======================================;
                 ;-------------CLOCK_CYCLE---------------;
                 ;=======================================;
                 ;#######################################;
                 										;
                 CLOCK_CYCLE:							;	=============================
000034 b6cf      	in saveSR, SREG						;	= Checks on every clock		=
000035 d00a      	rcall swcheck
000036 d018      	rcall checkEditLevel				;	= * What Edit level its on	=
000037 3054      	cpi editLevel,4						;	= * Check if the clock is	=
000038 f420      	brsh incSeconda
000039 940c 003b 	jmp CLOCK_CYCLE2
                 CLOCK_CYCLE2:										;	=	running.				=
00003b becf      	out SREG, saveSR					;	=							=	
00003c 9518      	reti								;	=============================
                 
                 incSeconda:
00003d d066      	rcall incSecond
00003e 940c 003b 	jmp CLOCK_CYCLE2										;
                 ;=======================================;
                 ;--------------END LABEL----------------;
                 ;=======================================;
                 
                 
                 
                 swcheck:
000040 e186      	ldi temp, PINB
000041 3080      	cpi temp,0x00
000042 f419      	brne swpouched
000043 e060      	ldi sw0Counter,0
000044 e070      	ldi sw1Counter,0
000045 9508      	ret
                 
                 	swpouched: 
000046 3081      		cpi temp,0x01
000047 f019      		breq sw0pouched
000048 3082      		cpi temp,0x02
000049 f019      		breq sw1pouched
00004a 9508      		ret
                 		
                 		sw0pouched:
00004b 9563      		inc sw0Counter
00004c 9508      		ret
                 
                 		sw1pouched:
00004d 9573      		inc sw1Counter
00004e 9508      		ret
                 
                 
                 ;#######################################;
                 ;=======================================;
                 ;----------Check Edit Level-------------;
                 ;=======================================;
                 ;#######################################;
                 										;
                 checkEditLevel:							;	=============================
00004f 3050      	cpi editLevel,0
000050 f099      	breq startupa
000051 3051      	cpi editLevel,1
000052 f099      	breq setHoura
000053 3052      	cpi editLevel,2
000054 f099      	breq setMinutea
000055 3053      	cpi editLevel,3
000056 f099      	breq setSeconda
000057 3054      	cpi editLevel,4
000058 f099      	breq setAlarmStartupa
000059 3055      	cpi editLevel,5
00005a f099      	breq setAlarmHoura
00005b 3056      	cpi editLevel,6
00005c f099      	breq setAlarmMinutea
00005d 3057      	cpi editLevel,7
00005e f099      	breq playNoAlarma
00005f 3058      	cpi editLevel,8
000060 f099      	breq playYesAlarma
000061 3059      	cpi editLevel,9
000062 f099      	breq playNoAlarmAgaina
